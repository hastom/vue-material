{
  "name": "vue-material",
  "description": "Material Design for Vue.js",
  "version": "1.0.0-beta-14",
  "license": "MIT",
  "author": {
    "name": "marcosmoura",
    "email": "marcosvmmoura@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuematerial/vue-material.git"
  },
  "bugs": {
    "url": "https://github.com/vuematerial/vue-material/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/vue-material.js",
  "module": "dist/vue-material.js",
  "unpkg": "dist/vue-material.min.js",
  "scripts": {
    "dev": "babel-node build/local",
    "build": "rm -rf dist/ && babel-node build/lib",
    "docs": "babel-node build/docs",
    "lint": "eslint --ext .js,.vue docs src build test --fix",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "githooks": "sh build/git-hooks/install.sh",
    "changelog": "babel-node build/generate-changelog.js",
    "release": "sh build/release.sh",
    "new-component": "babel-node build/new-component.js",
    "github-release": "github-release",
    "commit": "git-cz",
    "postinstall": "opencollective-postinstall"
  },
  "engines": {
    "node": ">= 7.0"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "dependencies": {
    "opencollective-postinstall": "^2.0.2",
    "vue-github-button": "^1.2.0",
    "vue-github-buttons": "^3.1.0",
    "vue-toc": "0.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "autoprefixer": "^9.8.2",
    "avoriaz": "^6.3.0",
    "axios": "^0.19.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.0.0",
    "chalk": "^4.1.0",
    "clipboard": "^2.0.6",
    "codesandbox": "^2.1.16",
    "commitizen": "^4.1.2",
    "concat": "^1.0.3",
    "connect-history-api-fallback": "^1.5.0",
    "conventional-changelog": "^3.1.21",
    "conventional-changelog-vue-material": "^0.0.1",
    "copy-webpack-plugin": "^6.0.2",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "css-mqpacker": "^7.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "date-fns": "^2.0.0-alpha.27",
    "deepmerge": "^4.2.2",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-deep-equal": "^3.1.3",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fuzzysearch": "^1.0.3",
    "github-release-cli": "^1.3.1",
    "highlight.js": "^10.1.1",
    "html-webpack-plugin": "^4.3.0",
    "is-firefox": "^1.0.3",
    "is-promise": "^4.0.0",
    "jest": "^26.0.1",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "optimize-js-plugin": "^0.0.4",
    "ora": "^4.0.4",
    "popper.js": "^1.16.1",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prerender-spa-plugin": "^3.4.0",
    "pretty": "^2.0.0",
    "raf": "^3.4.1",
    "raw-loader": "^4.0.1",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.2",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "vue": "^2.6.11",
    "vue-ga": "^1.1.0",
    "vue-i18n": "^8.18.2",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.2",
    "vue-router": "^3.3.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuelidate": "^0.7.5",
    "vuex": "^3.4.0",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/vue-material"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "<rootDir>/node_modules/vue/dist/vue.common.js",
      "^vue-material/(.*)$": "<rootDir>/src/$1",
      "^test/(.*)$": "<rootDir>/test/$1",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^theme/(.*)$": "<rootDir>/src/theme/$1",
      "^base/(.*)$": "<rootDir>/src/base/$1",
      "^core/(.*)$": "<rootDir>/src/core/$1",
      "^components/(.*)$": "<rootDir>/src/components/$1",
      "\\.(css|scss)$": "<rootDir>/test/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/**/index.js",
      "!src/**/*.test.js"
    ],
    "coverageReporters": [
      "lcov",
      "json",
      "html"
    ],
    "testURL": "http://localhost:8080"
  },
  "browserslist": [
    "> 1%"
  ]
}
